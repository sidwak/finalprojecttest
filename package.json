{
  "name": "nodexify",
  "version": "0.0.1",
  "type": "module",
  "author": "Siddeshwar",
  "description": "final year project",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "electron": "electron .",
    "dev:electron": "concurrently \"npm run dev\" \"npm run electron\"",
    "etron": "npx tsc --project electron_main/tsconfig.json && npm run electron",
    "electron_tsc": "npx tsc --project electron_main/tsconfig.json",
    "app": "concurrently \"npm run dev\" \"npm run etron\""
  },
  "main": "./dist/main.js",
  "dependencies": {
    "@primevue/themes": "^4.2.4",
    "@vue-flow/background": "^1.3.2",
    "@vue-flow/controls": "^1.1.2",
    "@vue-flow/core": "^1.41.5",
    "chai": "^5.1.2",
    "pinia": "^2.3.0",
    "primeicons": "^7.0.0",
    "primevue": "^4.2.4",
    "puppeteer": "^24.2.0",
    "puppeteer-core": "^23.10.1",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "tailwindcss-primeui": "^0.3.4",
    "vue": "^3.5.12",
    "vue-router": "^4.4.5",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.0",
    "@types/chai": "^5.0.1",
    "@types/node": "^22.10.2",
    "@types/ws": "^8.5.13",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vitejs/plugin-vue-jsx": "^4.0.1",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.1.3",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.0",
    "electron": "^33.2.1",
    "electron-builder": "^25.1.8",
    "eslint": "^9.14.0",
    "eslint-plugin-vue": "^9.30.0",
    "npm-run-all2": "^7.0.1",
    "postcss": "^8.4.49",
    "postcss-import": "^16.1.0",
    "prettier": "^3.3.3",
    "sass-embedded": "^1.81.0",
    "tailwindcss": "^3.4.15",
    "typescript": "^5.7.2",
    "vite": "^5.4.10",
    "vite-plugin-vue-devtools": "^7.5.4",
    "vue-tsc": "^2.1.10"
  },
  "build": {
    "appId": "com.siddeshwar.nodexify",
    "productName": "Nodexify",
    "copyright": "Copyright Â© 2022 Siddeshwar",
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    },
    "files": [
      "dist/**/*"
    ],
    "directories": {
      "output": "dist_electron"
    }
  }
}
